          "_customHTML": "<section group=\"Additional Blocks\" class=\"footer1\" mbr-class=\"{'mbr-reveal': reveal, 'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"15\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"15\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Show Line\" name=\"showLine\" checked>\n        <header>Colors</header>\n        <input type=\"color\" title=\"Line Color\" name=\"lineColor\" value=\"#4f87fb\" selected condition=\"showLine\">\n        <input type=\"color\" title=\"Icons Color\" name=\"iconcolor\" value=\"#ffffff\" selected>\n        <input type=\"color\" title=\"Icons Bg Color\" name=\"iconBgcolor\" value=\"#4f87fb\" selected>\n        <input type=\"color\" title=\"Link Icons Color\" name=\"linkiconcolor\" value=\"#344cf1\" selected>\n        <input type=\"color\" title=\"Days Color\" name=\"daysColor\" value=\"#6d7a8c\" selected>\n        <input type=\"color\" title=\"Times Color\" name=\"timesColor\" value=\"#6d7a8c\" selected>\n        <hr>\n        <input type=\"range\" inline title=\"Items\" name=\"items\" min=\"1\" max=\"5\" step=\"1\" value=\"1\">\n        <input type=\"range\" inline title=\"Links\" name=\"links\" min=\"1\" max=\"5\" step=\"1\" value=\"4\">\n        <input type=\"range\" inline title=\"Hours Items\" name=\"hours\" min=\"1\" max=\"8\" step=\"1\" value=\"1\">\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#ffffff\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#232323\" condition=\"overlay&&bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay&&bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&&bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div class=\"container\">\n        <div class=\"media-container-row content text-white\">\n\n            <div class=\"col-12 col-md-6 col-lg-3\">\n                <h5 class=\"align-left widget-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".widget-title, .line-wrap\">\n                    ABOUT</h5>\n                <div class=\"line-wrap align-left\" mbr-if=\"showLine\">\n                    <div class=\"line\"></div>\n                </div>\n                <p class=\"mbr-text mbr-normal mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">A Global Action Campaign to Reduce the Risks of Pandemics by Protecting and Regenerating Nature<br></p>\n                <div class=\"item\" mbr-if=\"items > 0\">\n                    <div class=\"card-img\"><span mbr-icon class=\"mbr-iconfont img1 fa-hand-peace-o fa\"></span>\n                    </div>\n                    <div class=\"card-box\">\n                        <h4 class=\"item-title align-left mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-title\">#EndPandemics</h4>\n                    </div>\n                </div>\n                <div class=\"item\" mbr-if=\"items > 1\">\n                    <div class=\"card-img\"><span mbr-icon class=\"mbr-iconfont img1 mdi-communication-phone\"></span></div>\n                    <div class=\"card-box\">\n                        <h4 class=\"item-title align-left mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-title\">Phone: +(999) 5554432</h4>\n                    </div>\n                </div>\n                <div class=\"item\" mbr-if=\"items > 2\">\n                    <div class=\"card-img\"><span mbr-icon class=\"mbr-iconfont img1 mobi-mbri-letter mobi-mbri\"></span>\n                    </div>\n                    <div class=\"card-box\">\n                        <h4 class=\"item-title align-left mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-title\">support@yourmail.com</h4>\n                    </div>\n                </div>\n                <div class=\"item\" mbr-if=\"items > 3\">\n                    <div class=\"card-img\"><span mbr-icon class=\"mbr-iconfont img1 mobi-mbri-phone mobi-mbri\"></span>\n                    </div>\n                    <div class=\"card-box\">\n                        <h4 class=\"item-title align-left mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-title\">+1 (0) 000 0000 001</h4>\n                    </div>\n                </div>\n                <div class=\"item\" mbr-if=\"items > 4\">\n                    <div class=\"card-img\"><span mbr-icon class=\"mbr-iconfont img1 mobi-mbri-phone mobi-mbri\"></span>\n                    </div>\n                    <div class=\"card-box\">\n                        <h4 class=\"item-title align-left mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".item-title\">+1 (0) 000 0000 001</h4>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-6 col-lg-3\">\n                <h5 class=\"align-left widget-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".widget-title, .line-wrap\">\n                    ALLIANCE</h5>\n                <div class=\"line-wrap align-left\" mbr-if=\"showLine\">\n                    <div class=\"line\"></div>\n                </div>\n\n                <div class=\"link\" mbr-if=\"links > 0\">\n                    <div class=\"card-img\"><span mbr-icon class=\"mbr-iconfont img2 mobi-mbri-arrow-next mobi-mbri\"></span>\n                    </div>\n                    <div class=\"card-box\">\n                        <h4 class=\"link-title align-left mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".link-title\"><a href=\"index.html#faq-strongertogether\" class=\"mbr-fonts-style text-info\" mbr-theme-style=\"display-4\">Who We Are</a></h4>\n                    </div>\n                </div>\n                <div class=\"link\" mbr-if=\"links > 1\">\n                    <div class=\"card-img\"><span mbr-icon class=\"mbr-iconfont img2 mobi-mbri-arrow-next mobi-mbri\"></span></div>\n                    <div class=\"card-box\">\n                        <h4 class=\"link-title align-left mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".link-title\"><a href=\"index.html#action-pillars\" class=\"text-white\"></a><a href=\"index.html#action-pillars\" class=\"text-info\">Action Pillars</a></h4>\n                    </div>\n                </div>\n                <div class=\"link\" mbr-if=\"links > 2\">\n                    <div class=\"card-img\"><span mbr-icon class=\"mbr-iconfont img2 mobi-mbri-arrow-next mobi-mbri\"></span>\n                    </div>\n                    <div class=\"card-box\">\n                        <h4 class=\"link-title align-left mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".link-title\"><a href=\"http://tiny.cc/ep-terms\" target=\"_blank\" class=\"text-info\">Mission &amp; Terms of Engagement</a></h4>\n                    </div>\n                </div>\n\n                <div class=\"link\" mbr-if=\"links > 3\">\n                    <div class=\"card-img\"><span mbr-icon class=\"mbr-iconfont img2 mobi-mbri-arrow-next mobi-mbri\"></span>\n                    </div>\n                    <div class=\"card-box\">\n                        <h4 class=\"link-title align-left mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".link-title\"><a href=\"presskit.html#info1-2q\" class=\"text-info\">Press Kit</a></h4>\n                    </div>\n                </div>\n\n                <div class=\"link\" mbr-if=\"links > 4\">\n                    <div class=\"card-img\"><span mbr-icon class=\"mbr-iconfont img2 mobi-mbri-arrow-next mobi-mbri\"></span>\n                    </div>\n                    <div class=\"card-box\">\n                        <h4 class=\"link-title align-left mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".link-title\"><a href=\"https://mobirise.com/\" class=\"text-white\">Faq</a>\n                        </h4>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-6 col-lg-3\">\n                <h5 class=\"align-left widget-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".widget-title, .line-wrap\">RECENT MEDIA</h5>\n                <div class=\"line-wrap align-left\" mbr-if=\"showLine\">\n                    <div class=\"line\"></div>\n                </div>\n                <div class=\"list\">\n                    <div class=\"list-item d-flex\">\n                        <img class=\"tips\" src=\"../_images/img/01.jpg\" alt=\"Mobirise\">\n                        <div class=\"content d-flex flex-column justify-content-center\">\n                            <h4 mbr-theme-style=\"display-4\" class=\"link-wrap mbr-fonts-style\" data-app-selector=\".list .list-item .content .link-wrap\"><a href=\"https://www.youtube.com/watch?v=MzYlngLMlcg\" target=\"_blank\" class=\"text-primary\">Another Wuhan in the Making</a></h4>\n                            <h4 mbr-theme-style=\"display-4\" class=\"date-wrap mbr-fonts-style\" data-app-selector=\".list .list-item .content .date-wrap\">29 April 2020\n                            </h4>\n                        </div>\n                    </div>\n                    <div class=\"list-item d-flex\">\n                        <a href=\"https://youtu.be/97qVephsYyo\"><img class=\"tips\" src=\"@PROJECT_PATH@/assets/images/media-thumbnail-124x133.png\" alt=\"Global Webinar\" title=\"Global Webinar\"></a>\n                        <div class=\"content d-flex flex-column justify-content-center\">\n                            <h4 mbr-theme-style=\"display-4\" class=\"link-wrap mbr-fonts-style\" data-app-selector=\".list .list-item .content .link-wrap\"><a href=\"https://youtu.be/97qVephsYyo\" target=\"_blank\" class=\"text-primary\">Global Webinar</a></h4><h4 mbr-theme-style=\"display-4\" class=\"date-wrap mbr-fonts-style\" data-app-selector=\".list .list-item .content .date-wrap\">20 April 2020</h4>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-md-6 col-lg-3\">\n                <h5 class=\"align-left widget-title mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".widget-title, .line-wrap\">UPCOMING EVENTS</h5>\n                <div class=\"line-wrap align-left\" mbr-if=\"showLine\">\n                    <div class=\"line\"></div>\n                </div>\n                <div class=\"box-list mbr-fonts-style\" mbr-list mbr-theme-style=\"display-4\" data-app-selector=\".box-list li\">\n                    <li class=\"clearfix\" mbr-text mbr-if=\"hours>0\">Wed (weekly call)<span>14:30 UTC</span></li>\n                    <li class=\"clearfix\" mbr-text mbr-if=\"hours>1\">Wed - Thur<span>09.30 - 16.30</span></li>\n                    <li class=\"clearfix\" mbr-text mbr-if=\"hours>2\">Fri - Sat<span>11.30 - 20.00</span></li>\n                    <li class=\"clearfix\" mbr-text mbr-if=\"hours>3\">Saturday<span>10.30 - 12.00</span></li>\n                    <li class=\"clearfix\" mbr-text mbr-if=\"hours>4\">Sunday<span>Closed</span></li>\n                    <li class=\"clearfix\" mbr-
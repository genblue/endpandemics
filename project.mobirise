alue=\"https://www.youtube.com/watch?v=E0Pa8tYo94U\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay&amp;&amp;bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"col-lg-4\">\n                <div class=\"box-item mbr-white left\">\n                    <div class=\"wrap\">\n                        <div class=\"headingicon d-flex flex-column align-items-start\">\n                            <div class=\"icon-block-top\" mbr-if=\"showIcons\">\n                                <span mbr-icon=\"\" class=\"mbr-iconfont mdi-communication-phone\"></span>\n                            </div>\n                            <div class=\"heading\" mbr-if=\"showTitle\">\n                                <h4 class=\"box-item-title pb-3 mbr-fonts-style mbr-bold\" data-app-selector=\".icon-block-top, .box-item-title, .icon-block-top\" mbr-theme-style=\"display-7\">Stay Connected\n</h4>\n                            </div>\n                        </div>\n                        <p class=\"box-item-text mbr-fonts-style\" data-app-selector=\".box-item-text, .mbr-section-btn\" mbr-theme-style=\"display-7\" mbr-if=\"showText\">\n                            Our hospital offers state-of-the-art comprehensive care and dedicated services for patients from around the world.</p>\n                        <div mbr-buttons=\"\" mbr-theme-style=\"display-4\" class=\"mbr-section-btn pb-4\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\">\n                            <a class=\"btn btn-lg btn-white-outline btn-bgr\" href=\"https://mobirise.com\">SEE SERVICES</a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-lg-4\">\n                <div class=\"box-item mbr-white\">\n                    <div class=\"wrap\">\n                        <div class=\"headingicon d-flex flex-column align-items-start\">\n                            <div class=\"icon-block-top\" mbr-if=\"showIcons\">\n                                <span mbr-icon=\"\" class=\"mbr-iconfont mdi-maps-directions-walk\"></span>\n                            </div>\n                            <div class=\"heading\" mbr-if=\"showTitle\">\n                                <h4 class=\"box-item-title pb-3 mbr-fonts-style mbr-bold\" data-app-selector=\".icon-block-top, .box-item-title, .icon-block-top\" mbr-theme-style=\"display-7\">#1 Hospital on the West Coast</h4>\n                            </div>\n                        </div>\n                        <p class=\"box-item-text mbr-fonts-style\" data-app-selector=\".box-item-text, .mbr-section-btn\" mbr-theme-style=\"display-7\" mbr-if=\"showText\">We bridge innovation science with state-of-the-art clinical medicine.\n<br></p>\n                        <div mbr-buttons=\"\" mbr-theme-style=\"display-4\" class=\"mbr-section-btn pb-4\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\">\n                            <a class=\"btn btn-lg btn-white-outline btn-bgr\" href=\"https://mobirise.com\">LEARN MORE</a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-lg-4\">\n                <div class=\"box-item mbr-white right\">\n                    <div class=\"wrap\">\n                        <div class=\"headingicon d-flex flex-column align-items-start\">\n                            <div class=\"icon-block-top\" mbr-if=\"showIcons\">\n                                <span mbr-icon=\"\" class=\"mbr-iconfont display-2 mdi-av-subtitles\"></span>\n                            </div>\n                            <div class=\"heading\" mbr-if=\"showTitle\">\n                                <h4 class=\"box-item-title pb-3 mbr-fonts-style mbr-bold\" data-app-selector=\".icon-block-top, .box-item-title, .icon-block-top\" mbr-theme-style=\"display-7\">\n                                    Opening\n                                    Hours</h4>\n                            </div>\n                        </div>\n                        <ul class=\"box-list mbr-fonts-style\" mbr-list=\"\" mbr-theme-style=\"display-4\" mbr-if=\"showTimeTable\" data-app-selector=\".box-list li\">\n                            <li class=\"mbr-fonts-style clearfix\" mbr-text=\"\" data-app-selector=\".box-list li\" mbr-if=\"hours\">Monday -\n                                Friday\n                                <span>7.30am-8.30pm</span></li>\n                            <li class=\"mbr-fonts-style clearfix\" mbr-text=\"\" data-app-selector=\".box-list li\" mbr-if=\"hours>1\">Saturday\n                                <span>6.20am-5.30pm</span></li>\n                            <li class=\"mbr-fonts-style clearfix\" mbr-text=\"\" data-app-selector=\".box-list li\" mbr-if=\"hours>2\">Sunday\n                                <span>Closed</span></li>\n                            <li class=\"mbr-fonts-style clearfix\" mbr-text=\"\" data-app-selector=\".box-list li\" mbr-if=\"hours>3\">Sunday\n                                <span>Closed</span></li>\n                            <li class=\"mbr-fonts-style clearfix\" mbr-text=\"\" data-app-selector=\".box-list li\" mbr-if=\"hours>4\">Sunday\n                                <span>Closed</span></li>\n                            <li class=\"mbr-fonts-style clearfix\" mbr-text=\"